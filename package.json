{
  "name": "janbee-cv",
  "version": "1.0.27",
  "homepage": "https://janbeeangel.es/build",
  "private": true,
  "scripts": {
    "rm": "rm -rf node_modules && rm yarn.lock || rd /s /q node_modules",
    "yrn:update": "yarn upgrade-interactive --latest",
    "i": "yarn install",
    "prettier:format": "prettier --write './**/*.{jsx,ts,tsx,css,md,json}'",
    "prepare:husky": "rm -rf .husky & npx husky install && npx husky add .husky/pre-commit \"npm --no-git-tag-version version patch && git add . && yarn build && npx --no-install lint-staged\"",
    "------------------ ----------------------- ------------------": "",
    "start": "craco start",
    "build": "craco build && cp ./build/index.html ./",
    "test": "craco test",
    "------------------ git ------------------": "",
    "git:add": "git add .",
    "git:commit": "git commit -a -m 'auto'",
    "git:push": "git push",
    "git:run": "yarn run git:add && yarn run git:commit && yarn run git:push"
  },
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "rxjs": "^7.5.6",
    "sass": "^1.54.4",
    "ts-jest": "^28.0.7",
    "use-font-face-observer": "^1.2.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/gapi": "^0.0.42",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "craco-alias": "^3.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "semantic-ui-react": "^2.0.4",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{jsx,ts,tsx,css,md,json}": [
      "git add .",
      "prettier --write './**/*.{ts,tsx}'"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
